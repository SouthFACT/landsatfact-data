CLASS
	NAME "-61 to -80"
	EXPRESSION ([pixel] >= -80 AND [pixel] <= -61)
	COLOR 255 128 0
END
CLASS
	NAME "-81 to -100"
	EXPRESSION ([pixel] >= -100 AND [pixel] <= -81)
	COLOR 255 0 0
END
CLASS
	NAME "-101 to -120"
	EXPRESSION ([pixel] <= -101 AND [pixel] >= -120)
	COLOR 163 0 0
END
CLASS
	NAME "< 120"
	EXPRESSION ([pixel] < -120)
	COLOR 110 0 0 
END